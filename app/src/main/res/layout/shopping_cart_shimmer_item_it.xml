<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/bag_container1"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_margin="5dp"
            android:background="@color/placeholder"
            android:textColor="@color/placeholder"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </FrameLayout>

        <TextView
            android:id="@+id/product_name_text1"
            style="@style/Product.Name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            android:background="@color/placeholder"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/poppins_regular"
            android:gravity="start"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:text="@string/product_name"
            android:textColor="@color/placeholder"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@id/product_description_text1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/bag_container1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/product_description_text1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            android:background="@color/placeholder"
            android:fontFamily="@font/poppins_light"
            android:gravity="start"
            android:maxLines="1"
            android:text="@string/product_name"
            android:textColor="@color/placeholder"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@id/product_price_container1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/bag_container1"
            app:layout_constraintTop_toBottomOf="@id/product_name_text1" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/product_price_container1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:background="@color/placeholder"
            android:textColor="@color/placeholder"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/bag_container1"
            app:layout_constraintTop_toBottomOf="@id/product_description_text1">

            <TextView
                android:id="@+id/symbol_money1"
                style="@style/Product.Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:background="@color/placeholder"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center_horizontal"
                android:textColor="@color/placeholder"
                android:textSize="9sp"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/product_price_text1"
                style="@style/Product.Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="20dp"
                android:background="@color/placeholder"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center_horizontal"
                android:textColor="@color/placeholder"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/symbol_money1"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/quantity1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:background="@color/placeholder"
            android:fontFamily="@font/poppins_bold"
            android:gravity="start"
            android:maxLines="1"
            android:textColor="@color/placeholder"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>